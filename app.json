{
  "expo": {
    "name": "zengroup2023",
    "slug": "zengroup2023",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/zengroup-01.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/zengroup-01.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/zengroup-01.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "zengroup2023"
  }
}
